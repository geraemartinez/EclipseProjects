package model;

import java.io.Serializable;
import javax.persistence.*;

import model.GroupsUserPK;


/**
 * The persistent class for the groups_users database table.
 * 
 */
@Entity
@Table(name="groups_users")
@NamedQuery(name="GroupsUser.findAll", query="SELECT g FROM GroupsUser g")
public class GroupsUser implements Serializable {
	private static final long serialVersionUID = 1L;

	@EmbeddedId
	private GroupsUserPK id;

	public GroupsUser() {
	}

	public GroupsUserPK getId() {
		return this.id;
	}

	public void setId(GroupsUserPK id) {
		this.id = id;
	}

}