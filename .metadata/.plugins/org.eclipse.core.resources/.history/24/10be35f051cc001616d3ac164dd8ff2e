package ctc.traccar.api.exceptions;

import javax.persistence.PersistenceException;

public class DataException extends PersistenceException {

	private static final long serialVersionUID = 1L;
	private TransactionStatus status;
	
	public TransactionStatus getStatus() {
		return status;
	}

	public void setStatus(TransactionStatus status) {
		this.status = status;
	}

	public DataException(String message,TransactionStatus status, Throwable t){
		super(message, t);
		this.status = status;
	}
	
}
